{
  "name": "oofem",
  "displayName": "oofem-input-file",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:oofem-input"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oofem-input-file.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "code-actions-sample.command",
        "title": "Emoji cmd"
      }
    ],
    "languages": [
      {
        "id": "oofem-input",
        "aliases": [
          "oofem"
        ],
        "extensions": [
          ".in"
        ]
      }
    ],
    "grammars": [
      {
        "language": "oofem-input",
        "scopeName": "source.oofem-input",
        "path": "./syntaxes/oofem-input.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "[link]": {
        "editor.bracketPairColorization.enabled": false
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
